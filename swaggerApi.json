{
  "openapi": "3.0.0",
  "info": {
    "version": "v1.0.0",
    "title": "E-commerce API",
    "description": "E-commerce API documentation"
  },
  "servers": [
    {
      "url": "http://localhost:4000/api",
      "description": "Local HTTP"
    },
    {
      "url": "https://localhost:4000/api",
      "description": "Local HTTPS"
    }
  ],
  "paths": {
    "/health_check": {
      "get": {
        "tags": [
          "Health Check"
        ],
        "description": "Health Check API",
        "responses": {
          "default": {
            "description": ""
          }
        }
      }
    },
    "/user": {
      "post": {
        "tags": [
          "User"
        ],
        "description": "Create User API",
        "responses": {
          "default": {
            "description": ""
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/createUser"
              }
            },
            "application/xml": {
              "schema": {
                "$ref": "#/components/schemas/createUser"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "schemas": {
      "createUser": {
        "type": "object",
        "properties": {
          "first_name": {
            "type": "string",
            "example": "john"
          },
          "last_name": {
            "type": "string",
            "example": "doe"
          },
          "email": {
            "type": "string",
            "example": "o1oYb@example.com"
          },
          "password": {
            "type": "string",
            "example": "password"
          },
          "address": {
            "type": "string",
            "example": "123 Main St"
          },
          "image": {
            "type": "string",
            "example": "https://example.com/image.png"
          },
          "phone": {
            "type": "string",
            "example": "1234567890"
          }
        },
        "required": [
          "first_name",
          "last_name",
          "email",
          "password",
          "address",
          "image",
          "phone"
        ],
        "xml": {
          "name": "createUser"
        }
      }
    }
  }
}